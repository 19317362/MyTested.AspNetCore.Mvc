{
  "buildOptions": {
    "warningsAsErrors": true,
    "preserveCompilationContext": true,
    "copyToOutput": {
      "include": [
        "testconfig.json"
      ]
    }
  },
  "dependencies": {
    "MSTest.TestFramework": "1.0.3-*",
    "dotnet-test-mstest": "1.1.1-*",
    "MyTested.AspNetCore.Mvc.Controllers": "1.1.0",
    "MyTested.AspNetCore.Mvc.DependencyInjection": "1.1.0",
    "MyTested.AspNetCore.Mvc.Models": "1.1.0",
    "MyTested.AspNetCore.Mvc.Routing": "1.1.0",
    "MyTested.AspNetCore.Mvc.ViewComponents": "1.1.0",
    "NoStartup.Controllers": "*",
    "NoStartup.Components": "*",
    "NoStartup.Services": "*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        }
      }
    },
    "net451": {}
  },
  "testRunner": "mstest"
}