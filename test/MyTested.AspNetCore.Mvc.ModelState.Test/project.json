{
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": {
      "include": [
        "testconfig.json"
      ]
    }
  },

  "testRunner": "xunit",

  "dependencies": {
    "Microsoft.DotNet.InternalAbstractions": "1.0.0", // remove when not needed
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.2.0-*",
    "MyTested.AspNetCore.Mvc.Controllers": "1.1.1",
    "MyTested.AspNetCore.Mvc.ModelState": "1.1.1",
    "MyTested.AspNetCore.Mvc.Models": "1.1.1",
    "MyTested.AspNetCore.Mvc.ViewComponents": "1.1.1",
    "MyTested.AspNetCore.Mvc.Test.Setups": "*"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dotnet5.4",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        }
      }
    },
    "net451": {}
  }
}
