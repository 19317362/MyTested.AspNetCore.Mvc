    "keyFile": "../../tools/Key.snk",
    "copyToOutput": {
      "include": [
        "testconfig.json"
      ]
    }
  },

  "testRunner": "xunit",

  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.2.0-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-*",
    "MyTested.AspNetCore.Mvc.Caching": "1.0.0-*",
    "MyTested.AspNetCore.Mvc.Test": "1.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      }
    },
    "net451": { }
